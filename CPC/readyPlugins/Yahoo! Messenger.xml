<CoEIA-Plugins>
<Metadata>
<Plugin-Name>Yahoo  Messenger</Plugin-Name>
<Plugin-Category>Chat and Instant Messaging History</Plugin-Category>
<Plugin-Version>1.0</Plugin-Version>
</Metadata>
<Data>
<Items>
<Item>
<Name>Yahoo  Messenger Cache and History Files</Name> 
<Objects>
<Object>
<Type>Folder</Type>
<Command>DeleteAllBellow</Command>
<Path>
<OS name="ALL">
<Location>%ProgramFiles%\Yahoo \Messenger\IMVCache</Location>
<Patterns>
<Pattern>*.*</Pattern>
</Patterns>
</OS>
</Path>
</Object>

<Object>
<Type>File</Type>
<Command>Delete</Command>
<Path>
<OS name="ALL">
<Location>%ProgramFiles%\Yahoo \Messenger</Location>
<Patterns>
<Pattern>ypager.log</Pattern>
</Patterns>
</OS>
</Path>
</Object>

</Objects> 
</Item> 
</Items> 
</Data> 
</CoEIA-Plugins>