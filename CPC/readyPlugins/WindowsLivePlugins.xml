<CoEIA-Plugins>
	<Metadata>
		<Plugin-Name>Windows Live Messenger</Plugin-Name>
		<Plugin-Category>Chat and Instant Messaging History</Plugin-Category>
		<Plugin-Version>1.0</Plugin-Version>
	</Metadata>
	
	<Data>
		<Items>
			<Item>
				<Name>Chat Logs and Received Files</Name>
				<Objects>
					<Object>
						<Type>Folder</Type>
						<Command>DeleteAllBellow</Command>
						
						<Path>
							<OS name="XP">
								<Location>%USERPROFILE%\My Documents\My Received Files</Location>
								<Patterns>
									<Pattern>*.*</Pattern>
								</Patterns>
							</OS>
							
							<OS name="SEVEN">
								<Location>%USERPROFILE%\Documents\My Received Files</Location>
								<Patterns>
									<Pattern>*.*</Pattern>
								</Patterns>
							</OS>
						</Path>
					</Object>
				</Objects>				
			</Item>
		</Items>
	</Data>
</CoEIA-Plugins>