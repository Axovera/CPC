<CoEIA-Plugins>
<Metadata>
<Plugin-Name>Windows Recent Network Drive List</Plugin-Name>
<Plugin-Category>Windows Cache and History</Plugin-Category>
<Plugin-Version>1.0</Plugin-Version>
</Metadata>
<Data>
<Items>
<Item>
<Name>Windows Recent Network Drive List</Name> 
<Objects>
<Object>
<Type>Key</Type>
<Command>Delete</Command>
<Path>
<OS name="ALL">
<Location>HKEY_CURRENT_USER\Network</Location>
<Patterns>
<Pattern>Recent</Pattern>
</Patterns>
</OS>
</Path>
</Object>

<Object>
<Type>Key</Type>
<Command>Delete</Command>
<Path>
<OS name="ALL">
<Location>HKEY_CURRENT_USER\Software\Microsoft\Windows\CurrentVersion\Explorer</Location>
<Patterns>
<Pattern>Map Network Drive MRU</Pattern>
</Patterns>
</OS>
</Path>
</Object>

</Objects> 
</Item> 
</Items> 
</Data> 
</CoEIA-Plugins>