<CoEIA-Plugins>
<Metadata>
<Plugin-Name>Google Toolbar</Plugin-Name>
<Plugin-Category>ToolBar Application History</Plugin-Category>
<Plugin-Version>1.0</Plugin-Version>
</Metadata>
<Data>
<Items>
<Item>
<Name>Google Toolbar Search History</Name> 
<Objects>
<Object>
<Type>Key</Type>
<Command>Delete</Command>
<Path>
<OS name="ALL">
<Location>HKEY_CURRENT_USER\Software\Google\NavClient\1.0</Location>
<Patterns>
<Pattern>History</Pattern>
</Patterns>
</OS>
</Path>
</Object>

<Object>
<Type>Key</Type>
<Command>Delete</Command>
<Path>
<OS name="ALL">
<Location>HKEY_CURRENT_USER\Software\Google\NavClient\1.1</Location>
<Patterns>
<Pattern>History</Pattern>
</Patterns>
</OS>
</Path>
</Object>

<Object>
<Type>Value</Type>
<Command>Delete</Command>
<Path>
<OS name="ALL">
<Location>HKEY_CURRENT_USER\Software\Google\NavClient\1.1\Options</Location>
<Patterns>
<Pattern>KillPopupCount</Pattern>
</Patterns>
</OS>
</Path>
</Object>

<Object>
<Type>Folder</Type>
<Command>DeleteAllBellow</Command>
<Path>
<OS name="ALL">
<Location>%AppData%\Google\Local Search History</Location>
<Patterns>
<Pattern>*.*</Pattern>
</Patterns>
</OS>
</Path>
</Object>

</Objects> 
</Item> 
</Items> 
</Data> 
</CoEIA-Plugins>