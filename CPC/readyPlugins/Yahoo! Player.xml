<CoEIA-Plugins>
<Metadata>
<Plugin-Name>Yahoo  Player</Plugin-Name>
<Plugin-Category>Chat and Instant Messaging History</Plugin-Category>
<Plugin-Version>1.0</Plugin-Version>
</Metadata>
<Data>
<Items>
<Item>
<Name>Yahoo  Player History Information</Name> 
<Objects>
<Object>
<Type>Key</Type>
<Command>Delete</Command>
<Path>
<OS name="ALL">
<Location>HKEY_CURRENT_USER\Software\Yahoo\YPlayer</Location>
<Patterns>
<Pattern>Recent URLs</Pattern>
</Patterns>
</OS>
</Path>
</Object>

<Object>
<Type>Value</Type>
<Command>Delete</Command>
<Path>
<OS name="ALL">
<Location>HKEY_CURRENT_USER\Software\Yahoo\YPlayer\Settings</Location>
<Patterns>
<Pattern>open initial dir</Pattern>
</Patterns>
</OS>
</Path>
</Object>

<Object>
<Type>File</Type>
<Command>Delete</Command>
<Path>
<OS name="ALL">
<Location>%ProgramFiles%\Yahoo \Player</Location>
<Patterns>
<Pattern>yplayer.ypl</Pattern>
</Patterns>
</OS>
</Path>
</Object>

</Objects> 
</Item> 
</Items> 
</Data> 
</CoEIA-Plugins>