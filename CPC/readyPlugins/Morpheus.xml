<CoEIA-Plugins>
<Metadata>
<Plugin-Name>Morpheus</Plugin-Name>
<Plugin-Category>File Sharing History</Plugin-Category>
<Plugin-Version>1.0</Plugin-Version>
</Metadata>
<Data>
<Items>
<Item>
<Name>Morpheus Recent File List and Download/Shared Files</Name> 
<Objects>
<Object>
<Type>Key</Type>
<Command>Delete</Command>
<Path>
<OS name="ALL">
<Location>HKEY_CURRENT_USER\Software\Morpheus\Morpheus</Location>
<Patterns>
<Pattern>Recent File List</Pattern>
</Patterns>
</OS>
</Path>
</Object>

<Object>
<Type>Folder</Type>
<Command>DeleteAllBellow</Command>
<Path>
<OS name="ALL">
<Location>%ProgramFiles%\Morpheus\Db</Location>
<Patterns>
<Pattern>*.*</Pattern>
</Patterns>
</OS>
</Path>
</Object>

<Object>
<Type>Folder</Type>
<Command>DeleteAllBellow</Command>
<Path>
<OS name="ALL">
<Location>%ProgramFiles%\Morpheus\My Shared Folder</Location>
<Patterns>
<Pattern>*.*</Pattern>
</Patterns>
</OS>
</Path>
</Object>

</Objects> 
</Item> 
</Items> 
</Data> 
</CoEIA-Plugins>