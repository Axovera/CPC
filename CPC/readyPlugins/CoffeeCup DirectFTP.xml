<CoEIA-Plugins>
<Metadata>
<Plugin-Name>CoffeeCup DirectFTP</Plugin-Name>
<Plugin-Category>Download and Upload History</Plugin-Category>
<Plugin-Version>1.0</Plugin-Version>
</Metadata>
<Data>
<Items>
<Item>
<Name>CoffeeCup DirectFTP Server List and Recent Directory Information</Name> 
<Objects>
<Object>
<Type>Value</Type>
<Command>Delete</Command>
<Path>
<OS name="ALL">
<Location>HKEY_CURRENT_USER\Software\Idyle Software\DirectFTP\Interface</Location>
<Patterns>
<Pattern>localdir</Pattern>
</Patterns>
</OS>
</Path>
</Object>

<Object>
<Type>Value</Type>
<Command>Delete</Command>
<Path>
<OS name="ALL">
<Location>HKEY_CURRENT_USER\Software\Idyle Software\DirectFTP\Interface</Location>
<Patterns>
<Pattern>localpath</Pattern>
</Patterns>
</OS>
</Path>
</Object>

<Object>
<Type>File</Type>
<Command>Delete</Command>
<Path>
<OS name="ALL">
<Location>%ProgramFiles%\CoffeeCup Software\DirectFTP</Location>
<Patterns>
<Pattern>FTPServers.ini</Pattern>
</Patterns>
</OS>
</Path>
</Object>

</Objects> 
</Item> 
</Items> 
</Data> 
</CoEIA-Plugins>