<CoEIA-Plugins>
<Metadata>
<Plugin-Name>Xing MP3 Player</Plugin-Name>
<Plugin-Category>Multimedia History</Plugin-Category>
<Plugin-Version>1.0</Plugin-Version>
</Metadata>
<Data>
<Items>
<Item>
<Name>Xing MP3 Player Recent File List</Name> 
<Objects>
<Object>
<Type>ValueList</Type>
<Command>DeleteMatch</Command>
<Path>
<OS name="ALL">
<Location>HKEY_CURRENT_USER\Software\Xing Technology Corp.\XingMP3 Player</Location>
<Patterns>
<Pattern>srecentfile</Pattern>
</Patterns>
</OS>
</Path>
</Object>

<Object>
<Type>Value</Type>
<Command>Delete</Command>
<Path>
<OS name="ALL">
<Location>HKEY_CURRENT_USER\Xing Technology Corp.\XingMP3 Player</Location>
<Patterns>
<Pattern>splaylistfile</Pattern>
</Patterns>
</OS>
</Path>
</Object>

<Object>
<Type>File</Type>
<Command>Delete</Command>
<Path>
<OS name="ALL">
<Location>%ProgramFiles%\Xing\XingMP3 Player</Location>
<Patterns>
<Pattern>XingMP3Player.xpl</Pattern>
</Patterns>
</OS>
</Path>
</Object>

</Objects> 
</Item> 
</Items> 
</Data> 
</CoEIA-Plugins>