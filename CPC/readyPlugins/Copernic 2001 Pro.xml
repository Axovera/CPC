<CoEIA-Plugins>
<Metadata>
<Plugin-Name>Copernic 2001 Pro</Plugin-Name>
<Plugin-Category>Thrid-Party Application History</Plugin-Category>
<Plugin-Version>1.0</Plugin-Version>
</Metadata>
<Data>
<Items>
<Item>
<Name>Copernic 2001 Pro Search History and Downloaded History</Name> 
<Objects>
<Object>
<Type>Folder</Type>
<Command>DeleteMatch</Command>
<Path>
<OS name="ALL">
<Location>%ProgramFiles%\Copernic 2001 Pro\Search Results</Location>
<Patterns>
<Pattern>*.*</Pattern>
</Patterns>
</OS>
</Path>
</Object>

<Object>
<Type>Folder</Type>
<Command>DeleteAllBellow</Command>
<Path>
<OS name="ALL">
<Location>%ProgramFiles%\Copernic 2001 Pro\data</Location>
<Patterns>
<Pattern>*.*</Pattern>
</Patterns>
</OS>
</Path>
</Object>

</Objects> 
</Item> 
</Items> 
</Data> 
</CoEIA-Plugins>