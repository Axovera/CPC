<CoEIA-Plugins>
<Metadata>
<Plugin-Name>Teleport Pro</Plugin-Name>
<Plugin-Category>Thrid-Party Application History</Plugin-Category>
<Plugin-Version>1.0</Plugin-Version>
</Metadata>
<Data>
<Items>
<Item>
<Name>Teleport Pro Downloaded History</Name> 
<Objects>
<Object>
<Type>Key</Type>
<Command>Delete</Command>
<Path>
<OS name="ALL">
<Location>HKEY_CURRENT_USER\Software\Tennyson Maxwell\Teleport Pro</Location>
<Patterns>
<Pattern>Recent File List</Pattern>
</Patterns>
</OS>
</Path>
</Object>

<Object>
<Type>Value</Type>
<Command>Delete</Command>
<Path>
<OS name="ALL">
<Location>HKEY_CURRENT_USER\Software\Tennyson Maxwell\Teleport Pro\User</Location>
<Patterns>
<Pattern>lastproject</Pattern>
</Patterns>
</OS>
</Path>
</Object>

<Object>
<Type>Folder</Type>
<Command>DeleteAllBellow</Command>
<Path>
<OS name="ALL">
<Location>%ProgramFiles%\Teleport Pro\Projects</Location>
<Patterns>
<Pattern>*.*</Pattern>
</Patterns>
</OS>
</Path>
</Object>

</Objects> 
</Item> 
</Items> 
</Data> 
</CoEIA-Plugins>