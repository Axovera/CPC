<CoEIA-Plugins>
<Metadata>
<Plugin-Name>GetRight</Plugin-Name>
<Plugin-Category>Download and Upload History</Plugin-Category>
<Plugin-Version>1.0</Plugin-Version>
</Metadata>
<Data>
<Items>
<Item>
<Name>GetRight Recent File List and URLs</Name> 
<Objects>
<Object>
<Type>Key</Type>
<Command>Delete</Command>
<Path>
<OS name="ALL">
<Location>HKEY_CURRENT_USER\Software\Headlight\GetRight</Location>
<Patterns>
<Pattern>MRU</Pattern>
</Patterns>
</OS>
</Path>
</Object>

<Object>
<Type>Key</Type>
<Command>Delete</Command>
<Path>
<OS name="ALL">
<Location>HKEY_CURRENT_USER\Software\Headlight\GetRight</Location>
<Patterns>
<Pattern>TypedURLS</Pattern>
</Patterns>
</OS>
</Path>
</Object>

<Object>
<Type>Key</Type>
<Command>Delete</Command>
<Path>
<OS name="ALL">
<Location>HKEY_CURRENT_USER\Software\Headlight\GetRight</Location>
<Patterns>
<Pattern>Recent File List</Pattern>
</Patterns>
</OS>
</Path>
</Object>

<Object>
<Type>File</Type>
<Command>Delete</Command>
<Path>
<OS name="ALL">
<Location>%ProgramFiles%\GetRight</Location>
<Patterns>
<Pattern>GetRight.hst</Pattern>
</Patterns>
</OS>
</Path>
</Object>

</Objects> 
</Item> 
</Items> 
</Data> 
</CoEIA-Plugins>