<CoEIA-Plugins>
<Metadata>
<Plugin-Name>Download Accelerator</Plugin-Name>
<Plugin-Category>Download and Upload History</Plugin-Category>
<Plugin-Version>1.0</Plugin-Version>
</Metadata>
<Data>
<Items>
<Item>
<Name>Download Accelerator Download URLs History</Name> 
<Objects>
<Object>
<Type>Value</Type>
<Command>Delete</Command>
<Path>
<OS name="ALL">
<Location>HKEY_CURRENT_USER\Software\SpeedBit\Download Accelerator\HistoryCombo</Location>
<Patterns>
<Pattern>URLHistory</Pattern>
</Patterns>
</OS>
</Path>
</Object>

<Object>
<Type>Folder</Type>
<Command>DeleteAllBellow</Command>
<Path>
<OS name="ALL">
<Location>%ProgramFiles%\DAP\Ads</Location>
<Patterns>
<Pattern>*.*</Pattern>
</Patterns>
</OS>
</Path>
</Object>

<Object>
<Type>Folder</Type>
<Command>DeleteAllBellow</Command>
<Path>
<OS name="ALL">
<Location>%ProgramFiles%\DAP\Temp</Location>
<Patterns>
<Pattern>*.*</Pattern>
</Patterns>
</OS>
</Path>
</Object>

</Objects> 
</Item> 
</Items> 
</Data> 
</CoEIA-Plugins>