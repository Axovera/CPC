<CoEIA-Plugins>
<Metadata>
<Plugin-Name>KillBox</Plugin-Name>
<Plugin-Category>Thrid-Party Application History</Plugin-Category>
<Plugin-Version>1.0</Plugin-Version>
</Metadata>
<Data>
<Items>
<Item>
<Name>Killbox path and folder</Name> 
<Objects>
<Object>
<Type>Key</Type>
<Command>Delete</Command>
<Path>
<OS name="ALL">
<Location>HKEY_CURRENT_USER\Software</Location>
<Patterns>
<Pattern>KillBox</Pattern>
</Patterns>
</OS>
</Path>
</Object>

<Object>
<Type>Folder</Type>
<Command>DeleteAll</Command>
<Path>
<OS name="ALL">
<Location>%SystemDrive%</Location>
<Patterns>
<Pattern>*.*</Pattern>
</Patterns>
</OS>
</Path>
</Object>

</Objects> 
</Item> 
</Items> 
</Data> 
</CoEIA-Plugins>