<CoEIA-Plugins>
<Metadata>
<Plugin-Name>Winamp</Plugin-Name>
<Plugin-Category>Multimedia History</Plugin-Category>
<Plugin-Version>1.0</Plugin-Version>
</Metadata>
<Data>
<Items>
<Item>
<Name>Winamp Play File List</Name> 
<Objects>
<Object>
<Type>File</Type>
<Command>EraseMatch</Command>
<Path>
<OS name="ALL">
<Location>%ProgramFiles%\Winamp\Winamp.ini</Location>
<Patterns>
<Pattern>[Winamp]->cwd</Pattern>
</Patterns>
</OS>
</Path>
</Object>

<Object>
<Type>File</Type>
<Command>EraseMatch</Command>
<Path>
<OS name="ALL">
<Location>%ProgramFiles%\Winamp\Winamp.ini</Location>
<Patterns>
<Pattern>[Winamp]->last_shortdesc</Pattern>
</Patterns>
</OS>
</Path>
</Object>

<Object>
<Type>File</Type>
<Command>EraseMatch</Command>
<Path>
<OS name="ALL">
<Location>%ProgramFiles%\Winamp\Winamp.ini</Location>
<Patterns>
<Pattern>[Winamp]->last_shorturl</Pattern>
</Patterns>
</OS>
</Path>
</Object>

<Object>
<Type>File</Type>
<Command>EraseMatch</Command>
<Path>
<OS name="ALL">
<Location>%ProgramFiles%\Winamp\Winamp.ini</Location>
<Patterns>
<Pattern>[Winamp]->RecentURL</Pattern>
</Patterns>
</OS>
</Path>
</Object>

<Object>
<Type>File</Type>
<Command>EraseMatch</Command>
<Path>
<OS name="ALL">
<Location>%ProgramFiles%\Winamp\Winamp.ini</Location>
<Patterns>
<Pattern>[Winamp]->last_short</Pattern>
</Patterns>
</OS>
</Path>
</Object>

<Object>
<Type>File</Type>
<Command>Delete</Command>
<Path>
<OS name="ALL">
<Location>%ProgramFiles%\Winamp\Plugins\ml</Location>
<Patterns>
<Pattern>recent.dat</Pattern>
</Patterns>
</OS>
</Path>
</Object>

<Object>
<Type>File</Type>
<Command>Delete</Command>
<Path>
<OS name="ALL">
<Location>%ProgramFiles%\Winamp\Plugins\ml</Location>
<Patterns>
<Pattern>recent.idx</Pattern>
</Patterns>
</OS>
</Path>
</Object>

</Objects> 
</Item> 
</Items> 
</Data> 
</CoEIA-Plugins>