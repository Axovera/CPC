<Path>
<OS name="ALL">
<Location>HKEY_CURRENT_USER\Software\Counterpane Systems\Password Safe\MRU</Location>
<Patterns>
<Pattern>Safe</Pattern>
</Patterns>
</OS>
</Path>
</Object>

</Objects> 
</Item> 
</Items> 
</Data> 
</CoEIA-Plugins>