<CoEIA-Plugins>
<Metadata>
<Plugin-Name>EditPlus 2</Plugin-Name>
<Plugin-Category>Thrid-Party Application History</Plugin-Category>
<Plugin-Version>1.0</Plugin-Version>
</Metadata>
<Data>
<Items>
<Item>
<Name>EditPlus 2 Recent File List</Name> 
<Objects>
<Object>
<Type>File</Type>
<Command>EraseMatch</Command>
<Path>
<OS name="ALL">
<Location>%ProgramFiles%\EditPlus 2\editplus.ini</Location>
<Patterns>
<Pattern>[Opions]->Workspace Path</Pattern>
</Patterns>
</OS>
</Path>
</Object>

<Object>
<Type>Key</Type>
<Command>Delete</Command>
<Path>
<OS name="ALL">
<Location>HKEY_CURRENT_USER\Software\ES-Computing\EditPlus 2</Location>
<Patterns>
<Pattern>Recent Dir List</Pattern>
</Patterns>
</OS>
</Path>
</Object>

<Object>
<Type>Key</Type>
<Command>Delete</Command>
<Path>
<OS name="ALL">
<Location>HKEY_CURRENT_USER\Software\ES-Computing\EditPlus 2</Location>
<Patterns>
<Pattern>Recent File List</Pattern>
</Patterns>
</OS>
</Path>
</Object>

</Objects> 
</Item> 
</Items> 
</Data> 
</CoEIA-Plugins>