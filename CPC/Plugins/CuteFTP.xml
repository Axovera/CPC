<CoEIA-Plugins>
<Metadata>
<Plugin-Name>CuteFTP</Plugin-Name>
<Plugin-Category>Download and Upload History</Plugin-Category>
<Plugin-Version>1.0</Plugin-Version>
</Metadata>
<Data>
<Items>
<Item>
<Name>CuteFTP Log Files and History Information</Name> 
<Objects>
<Object>
<Type>File</Type>
<Command>Delete</Command>
<Path>
<OS name="ALL">
<Location>%ProgramFiles%\GlobalSCAPE\CuteFTP</Location>
<Patterns>
<Pattern>sm.dat</Pattern>
</Patterns>
</OS>
</Path>
</Object>

<Object>
<Type>File</Type>
<Command>Delete</Command>
<Path>
<OS name="ALL">
<Location>%ProgramFiles%\GlobalSCAPE\CuteFTP</Location>
<Patterns>
<Pattern>sm.bak</Pattern>
</Patterns>
</OS>
</Path>
</Object>

<Object>
<Type>Folder</Type>
<Command>DeleteAllBellow</Command>
<Path>
<OS name="ALL">
<Location>%ProgramFiles%\GlobalSCAPE\CuteFTP\Log</Location>
<Patterns>
<Pattern>*.*</Pattern>
</Patterns>
</OS>
</Path>
</Object>

</Objects> 
</Item> 
</Items> 
</Data> 
</CoEIA-Plugins>