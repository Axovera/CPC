<CoEIA-Plugins>
<Metadata>
<Plugin-Name>MyWay Advertising</Plugin-Name>
<Plugin-Category>Thrid-Party Application History</Plugin-Category>
<Plugin-Version>1.0</Plugin-Version>
</Metadata>
<Data>
<Items>
<Item>
<Name>MyWay Advertising Program Files</Name> 
<Objects>
<Object>
<Type>Key</Type>
<Command>Delete</Command>
<Path>
<OS name="ALL">
<Location>HKEY_LOCAL_MACHINE\SOFTWARE</Location>
<Patterns>
<Pattern>MyWay</Pattern>
</Patterns>
</OS>
</Path>
</Object>

<Object>
<Type>Folder</Type>
<Command>DeleteAll</Command>
<Path>
<OS name="ALL">
<Location>%ProgramFiles%\MyWay</Location>
<Patterns>
<Pattern>*.*</Pattern>
</Patterns>
</OS>
</Path>
</Object>

</Objects> 
</Item> 
</Items> 
</Data> 
</CoEIA-Plugins>