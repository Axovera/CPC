<CoEIA-Plugins>
<Metadata>
<Plugin-Name>Mass Download</Plugin-Name>
<Plugin-Category>Thrid-Party Application History</Plugin-Category>
<Plugin-Version>1.0</Plugin-Version>
</Metadata>
<Data>
<Items>
<Item>
<Name>Mass Download History Information</Name> 
<Objects>
<Object>
<Type>Folder</Type>
<Command>DeleteMatch</Command>
<Path>
<OS name="ALL">
<Location>%ProgramFiles%\Mass Downloader\INDEX</Location>
<Patterns>
<Pattern>*.*</Pattern>
</Patterns>
</OS>
</Path>
</Object>

<Object>
<Type>File</Type>
<Command>Delete</Command>
<Path>
<OS name="ALL">
<Location>%ProgramFiles%\Mass Downloader</Location>
<Patterns>
<Pattern>history.bak</Pattern>
</Patterns>
</OS>
</Path>
</Object>

<Object>
<Type>File</Type>
<Command>Delete</Command>
<Path>
<OS name="ALL">
<Location>%ProgramFiles%\Mass Downloader</Location>
<Patterns>
<Pattern>history.dat</Pattern>
</Patterns>
</OS>
</Path>
</Object>

<Object>
<Type>File</Type>
<Command>Delete</Command>
<Path>
<OS name="ALL">
<Location>%ProgramFiles%\Mass Downloader</Location>
<Patterns>
<Pattern>massdown.dat</Pattern>
</Patterns>
</OS>
</Path>
</Object>

</Objects> 
</Item> 
</Items> 
</Data> 
</CoEIA-Plugins>