<CoEIA-Plugins>
<Metadata>
<Plugin-Name>Net Vampire 3.x</Plugin-Name>
<Plugin-Category>Thrid-Party Application History</Plugin-Category>
<Plugin-Version>1.0</Plugin-Version>
</Metadata>
<Data>
<Items>
<Item>
<Name>Net Vampire 3.x Jobs, Log and Site History</Name> 
<Objects>
<Object>
<Type>Folder</Type>
<Command>DeleteMatch</Command>
<Path>
<OS name="ALL">
<Location>%ProgramFiles%\Net Vampire\Data\Jobs</Location>
<Patterns>
<Pattern>*.*</Pattern>
</Patterns>
</OS>
</Path>
</Object>

<Object>
<Type>Folder</Type>
<Command>DeleteMatch</Command>
<Path>
<OS name="ALL">
<Location>%ProgramFiles%\Net Vampire\Data\Sites</Location>
<Patterns>
<Pattern>*.*</Pattern>
</Patterns>
</OS>
</Path>
</Object>

<Object>
<Type>Folder</Type>
<Command>DeleteMatch</Command>
<Path>
<OS name="ALL">
<Location>%ProgramFiles%\Net Vampire\Data\State</Location>
<Patterns>
<Pattern>*.*</Pattern>
</Patterns>
</OS>
</Path>
</Object>

</Objects> 
</Item> 
</Items> 
</Data> 
</CoEIA-Plugins>