<CoEIA-Plugins>
<Metadata>
<Plugin-Name>Microsoft Word 2000</Plugin-Name>
<Plugin-Category>MS-Office History</Plugin-Category>
<Plugin-Version>1.0</Plugin-Version>
</Metadata>
<Data>
<Items>
<Item>
<Name>Microsoft Word 2000 Recent File List</Name> 
<Objects>
<Object>
<Type>Value</Type>
<Command>Delete</Command>
<Path>
<OS name="ALL">
<Location>HKEY_CURRENT_USER\Software\Microsoft\Office\9.0\Word\Data</Location>
<Patterns>
<Pattern>Settings</Pattern>
</Patterns>
</OS>
</Path>
</Object>

<Object>
<Type>Folder</Type>
<Command>DeleteAllBellow</Command>
<Path>
<OS name="ALL">
<Location>%AppData%\Microsoft\Office\Recent</Location>
<Patterns>
<Pattern>*.*</Pattern>
</Patterns>
</OS>
</Path>
</Object>

</Objects> 
</Item> 
</Items> 
</Data> 
</CoEIA-Plugins>